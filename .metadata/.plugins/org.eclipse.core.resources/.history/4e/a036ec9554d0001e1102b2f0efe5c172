package org.kh.site2;
//대입연산 2
public class Operation7 {

	public static void main(String[] args) {
		int a = 20;
		int b = 15;
		int tmp;
		
		a +=b;
		b -=a;
		
		//a<->b
		tmp = a;
		a = b;
		b = tmp;
		System.out.println("a="+a+", b="+b);
		
		int x = 0b01101;
		int y = 0b01011;
		
		//2진수 출력
		System.out.println("x="+Integer.toBinaryString(x));
		System.out.println("y="+Integer.toBinaryString(y));
		
		//비트 연산
		

	}

}
