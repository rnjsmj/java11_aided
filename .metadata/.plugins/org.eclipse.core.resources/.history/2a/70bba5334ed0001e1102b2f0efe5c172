package org.kh.site2;

//논리연산 : &&(AND), ||(OR), !(NOT),  
public class Operation6 {

	public static void main(String[] args) {
		int a = 26;
		int b = 28;
		int c = 27;
		
		//and 논리 : 모든 입력이 true일 경우 true
		System.out.println("a > b && a > c : "+(a>b && a>c)); //f && f
		System.out.println("a > b && b > c : "+(a>b && b>c)); //f && t
		System.out.println("a < b && b < c : "+(a<b && b<c)); //t && f
		System.out.println("a < b && b > c : "+(a<b && b>c)); //t && t
	}

}
