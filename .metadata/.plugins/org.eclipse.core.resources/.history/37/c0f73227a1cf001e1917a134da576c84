package org.kh.java;

public class Variable4 {
	public static void main(String[] args) {
		String name = "권민지";
		short age = 24;
		float height = 161.7f;
		double weight = 65.0;
		char init = 'k';
		
		/*
		 * 이스케이프 문자
		 * \n : 줄바꿈
		 * \t : Tab
		 * \\ : '\' 출력
		 */
		
		String name2 = "\'권민지\'";
		System.out.println("이름 : "+ name2);
		System.out.println("이름 : \"" +name+ "\"");
	}
}
